<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="roundSelector" format="reference" />
    <attr name="iconColor" format="color" />
    <attr name="toolbarPopupTheme" format="reference" />
    <attr name="dividerColor" format="color" />
    <attr name="rectSelector" format="reference" />

    <declare-styleable name="CardPanelView">
        <attr name="cpv_cardPosition" format="integer" />
        <attr name="cpv_cardCount" format="integer" />
        <attr name="cpv_cardImageRes" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="ColorIconsImageView">
        <attr name="iconBackgroundColor" format="color" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="civ_border" format="boolean" />
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_shadow" format="boolean" />
        <attr name="civ_shadow_color" format="color" />
        <attr name="civ_shadow_radius" format="float" />
    </declare-styleable>

</resources>